{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "fim-blocks/google-embed-pdf",
	"version": "2.0.1",
	"title": "FIM Google Embed",
	"category": "embed",
	"icon": "media-document",
	"description": "Embed a Google Document from a public-viewable link",
	"supports": {
		"html": false
	},
	"attributes":{
		"embedURL": {
			"type": "string",
			"selector": "value",
			"default": null
		},
		"heightFullscreen": {
			"type": "boolean",
			"selector": "checked"
		},
		"embedHeight": {
			"type": "string",
			"selector": "value",
			"default": "1200"
		}
	},
	"textdomain": "fim-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
